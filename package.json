{
  "name": "mcp-gearbox",
  "version": "0.0.10",
  "description": "A command-line interface tool for MCP server management and project initialization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-cli": "./dist/bin/mcp-cli.js",
    "mcp": "./dist/bin/mcp.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
