  "repository": {
    "type": "git",
    "url": "https://github.com/rohitsoni007/mcp-gearbox-js"
  },
  "homepage": "https://github.com/rohitsoni007/mcp-gearbox-cli",
  "bugs": {
    "url": "https://github.com/rohitsoni007/mcp-gearbox-cli"
  },
