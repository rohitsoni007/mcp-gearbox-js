    "node": ">=20.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
